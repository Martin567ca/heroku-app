{
  "name": "y",
  "version": "1.0.0",
  "description": "Application",
  "main": "server.js",
  "scripts": {
    "build": "cd front && npm install && npm run build",
    "install-client": "cd front && npm install",
    "heroku-postbuild": "cd front && npm install --only=dev && NODE_ENV=development npm install --no-shrinkwrap && npm run build",
    "start": "concurrently -n 'server,client' -c 'red,green'  \"nodemon server.js\" \"npm run client\"",
    "client": "cd front && npm start"
  },
  "cacheDirectories": [
    "node_modules",
    "front/node_modules"
  ],
  "author": "Maci",
  "license": "ISC",
  "dependencies": {
    "bootstrap": "^4.6.0",
    "concurrently": "^5.3.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoimport": "^1.0.6",
    "mongoose": "^5.11.14",
    "mongosm": "0.0.5",
    "nodemon": "^2.0.7"
  }
}
